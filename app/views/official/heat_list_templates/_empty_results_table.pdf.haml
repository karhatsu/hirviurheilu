- if @race.sport_key == Sport::METSASTYSTRAP
  .empty-results-table.empty-results-table--shotgun
    - 5.times do |i|
      - 4.times do |j|
        .empty-results-table__cell= i * 5 + j + 1
      .empty-results-table__cell.empty-results-table__cell--space= i * 5 + 5
    .empty-results-table__cell.empty-results-table__cell--total Yht
- elsif @race.sport_key == Sport::METSASTYSHAULIKKO
  .empty-results-table.empty-results-table--shotgun
    - shot_count.times do |i|
      .empty-results-table__cell= i + 1
    .empty-results-table__cell.empty-results-table__cell--total Yht
- elsif @race.sport_key == Sport::NORDIC && @sub_sport == :trap
  .empty-results-table
    - shot_count.times do |i|
      .empty-results-table__ternary-cell
        .empty-results-table__cell= i + 1
        .empty-results-table__cell 4
        .empty-results-table__cell 2
        .empty-results-table__cell 0
    .empty-results-table__ternary-cell
      .empty-results-table__cell.empty-results-table__cell--total Yht
      .empty-results-table__cell.empty-results-table__cell--total 4
      .empty-results-table__cell.empty-results-table__cell--total 2
      .empty-results-table__cell.empty-results-table__cell--total 0
- else
  .empty-results-table
  - shot_count.times do |i|
    .empty-results-table__cell= i + 1
  .empty-results-table__cell.empty-results-table__cell--total Yht
