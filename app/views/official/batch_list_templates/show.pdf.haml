- shot_count = @race.sport.qualification_round_shot_count
%h2 #{t 'activerecord.models.batch.one'}:
- @race.competitors_per_batch.times.each do
  %h3.empty-results-competitor-header #{t 'activerecord.models.competitor.one'}:
  = render partial: 'official/batch_list_templates/empty_results_table', locals: { shot_count: shot_count }
